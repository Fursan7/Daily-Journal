<%- include('partials/header.ejs'); %>

<form action="https://daily-journal-main.herokuapp.com/compose">
    <input type="submit" value="Compose" />
</form>
<h1>Home</h1>
<p><%= home %></p>

<% posts.forEach(function(post) {  %> 

    <h1><%= post.title %> </h1>
    <p><%= post.body.substring(0, 300) %>... <a href="/posts/<%= post.title %>">Read More</a></p>
    <% console.log(post.title); %>  
<% }); %>  
